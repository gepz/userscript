  "exports": {
    "./*": {
      "import": "./lib/*/index.js",
      "require": "./lib/*/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prod": "run-s prod_lib",
    "prod_lib": "tsc --project tsconfig.lib.json && tsc-alias -p tsconfig.lib.json",
    "start": "pnpm dev"
  }
}
