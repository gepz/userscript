  "dependencies": {
    "@types/node": "20.11.20",
    "webpack": "^5.90.3",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.8.8"
