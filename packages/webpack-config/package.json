{
  "name": "@userscript/webpack-config",
  "version": "1.0.5",
  "description": "",
  "keywords": [],
  "license": "AGPL-3.0-or-later",
  "author": "",
  "exports": {
    "./*": {
      "import": "./lib/*/index.js",
      "require": "./lib/*/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "run-s build-lib",
    "build-lib": "tsc --project tsconfig.lib.json && tsc-alias -p tsconfig.lib.json",
    "start": "pnpm dev"
  },
  "dependencies": {
    "@types/node": "20.14.11",
    "webpack": "^5.93.0",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.8.10"
  },
  "peerDependencies": {
    "ts-loader": "^9.4.1",
    "webpack": "^5.88.2"
  }
}
